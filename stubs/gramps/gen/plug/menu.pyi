from typing import Any

class MenuOption:
    def get_value(self) -> Any: ...
    def set_help(self, text: str) -> None: ...

class EnumeratedListOption(MenuOption):
    def __init__(self, label: str, value: str) -> None: ...
    def add_item(self, value: str, label: str) -> None: ...

class StringOption(MenuOption):
    def __init__(self, label: str, text: str) -> None: ...

class NumberOption(MenuOption):
    def __init__(self, label: str, value: int, min: int, max: int) -> None: ...
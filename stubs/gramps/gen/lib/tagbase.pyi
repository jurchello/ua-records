from typing import Any

class TagBase:
    def __init__(self, source: Any | None = ...) -> None: ...
    def add_tag(self, tag: str) -> None: ...
    def get_referenced_tag_handles(self) -> list[tuple[str, str]]: ...
    def get_tag_list(self) -> list[str]: ...
    def remove_tag(self, tag: str) -> bool: ...
    def replace_tag_references(self, old_handle: str, new_handle: str) -> None: ...
    def serialize(self) -> tuple[Any, ...]: ...
    def set_tag_list(self, tag_list: list[str]) -> None: ...
    def unserialize(self, data: tuple[Any, ...]) -> None: ...
from typing import Any, TYPE_CHECKING

if TYPE_CHECKING:
    from .mediaref import MediaRef

class MediaBase:
    def __init__(self, source: Any | None = ...) -> None: ...
    def add_media_reference(self, media_ref: "MediaRef") -> None: ...
    def get_media_list(self) -> list["MediaRef"]: ...
    def has_media_reference(self, obj_handle: str) -> bool: ...
    def remove_media_references(self, obj_handle_list: list[str]) -> None: ...
    def replace_media_references(self, old_handle: str, new_handle: str) -> None: ...
    def serialize(self) -> tuple[Any, ...]: ...
    def set_media_list(self, media_ref_list: list["MediaRef"]) -> None: ...
    def unserialize(self, data: tuple[Any, ...]) -> None: ...